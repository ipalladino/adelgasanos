<section class="section-fouricons">
	<div class="container">
		<div class="row">
	    <div class="col-lg-12 heading" >
			<% if(obj.title) %>
	    	<h1><%= obj.title %></h1>
		  <% end %>
			<% if(obj.description) %>
				<p><%= raw obj.description %></p>
			<% end %>
	    </div>
		</div>
	  <div class="row iconsrow">
	    <div class="col-lg-4 col-sm-4 col-sm-offset-1 col-lg-offset-1" >
	      <i class="<%= obj.child_items[0].graphic %>"></i> <%= obj.child_items[0].description %>
	    </div>
			<div class="col-lg-4 col-sm-4 col-sm-offset-2 col-lg-offset-2" >
	      <i class="<%= obj.child_items[1].graphic %>"></i> <%= obj.child_items[1].description %>
	    </div>
	  </div>
		
		<div class="row iconsrow">
	    <div class="col-lg-4 col-sm-4 col-sm-offset-1 col-lg-offset-1" >
	      <i class="<%= obj.child_items[2].graphic %>"></i> <%= obj.child_items[2].description %>
	    </div>
			<div class="col-lg-4 col-sm-4 col-sm-offset-2 col-lg-offset-2" >
	      <i class="<%= obj.child_items[3].graphic %>"></i> <%= obj.child_items[3].description %>
	    </div>
	  </div
	</div>
</section>